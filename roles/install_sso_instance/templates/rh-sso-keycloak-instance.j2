apiVersion: keycloak.org/v1alpha1
kind: Keycloak
metadata:
  name: rhsso
  namespace: {{ rh_sso_operator_ns | default('sso') }}
  annotations:
    argocd.argoproj.io/sync-options: SkipDryRunOnMissingResource=true
  labels:
    app: {{ rh_sso_operator_ns | default('sso') }}
spec:
  instances: 1
  profile: RHSSO
  externalAccess:
    enabled: true